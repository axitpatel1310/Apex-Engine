{
    "version": 2,
    "builds": [
    {
    "src": "manage.py",
    "use": "@vercel/python",
    "config": {
      "maxLambdaSize": "50mb",
      "runtime": "python3.11",
      "buildCommand": "python manage.py collectstatic --noinput"
    }
    }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "manage.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "Apex_Engine.settings"
    } 
  }